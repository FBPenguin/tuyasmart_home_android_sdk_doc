{
  "plugins": [
    "js-sequence-diagram-full@>=0.3.1",
    "prism",
    "-highlight",
    "anchors",
    "splitter",
    "github",
    "github-buttons",
    "-sharing",
    "anchor-navigation-ex",
    "alerts",
    "ga"
  ],
  "pluginsConfig": {
    "prism": {
      "css": [
        "prismjs/themes/prism-okaidia.css"
      ],
      "lang": {
        "java": "java"
      },
      "ignore": [
        "sequence"
      ]
    },
    "github": {
      "url": "https://github.com/TuyaInc/tuyasmart_home_android_sdk"
    },
    "github-buttons": {
      "buttons": [{
        "user": "TuyaInc",
        "repo": "tuyasmart_home_android_sdk",
        "type": "star",
        "size": "small"
      }]
    },
    "sharing": {
      "all": []
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": false,
      "showGoTop": true    
    },
    "ga": {
        "token": "UA-161421048-1"
    }
  }
}